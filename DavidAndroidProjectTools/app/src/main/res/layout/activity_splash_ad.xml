<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:kelina="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/jumpId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dip"
        android:layout_marginTop="10dip"
        android:gravity="center"
        android:orientation="vertical">

        <davidandroidprojecttools.qq986945193.com.davidandroidprojecttools.view.RoundProgressBar
            android:id="@+id/roundProgressBar"
            kelina:roundColor="#22000000"
            kelina:roundProgressColor="#00ccff"
            kelina:roundWidth="2dip"
            kelina:textColor="#FFFFFF"
            kelina:textSize="12dip"
            android:layout_width="50dip"
            android:layout_height="50dip" />
    </LinearLayout>

    <davidandroidprojecttools.qq986945193.com.davidandroidprojecttools.view.CountDownView
        android:id="@+id/countDownView"
        kelina:background_color="#22000000"
        kelina:border_color="#55B8E2"
        kelina:border_width="2dp"
        kelina:text_size="12dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/jumpId"
        android:layout_marginRight="10dip"
        android:layout_marginTop="10dip" />
</LinearLayout>